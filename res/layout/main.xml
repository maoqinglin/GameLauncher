<?xml version="1.0" encoding="utf-8"?>
<com.ireadygo.app.gamelauncher.ui.widget.CustomFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/main_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipChildren="false" >

    <View
        android:id="@+id/focusView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left|top"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:nextFocusDown="@+id/menu_store"
        android:nextFocusLeft="@+id/menu_store"
        android:nextFocusRight="@+id/menu_store"
        android:nextFocusUp="@+id/menu_store" />

    <ImageView
        android:id="@+id/menu_highlight_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/menu_my_highlight"
        android:visibility="gone" />
</com.ireadygo.app.gamelauncher.ui.widget.CustomFrameLayout>